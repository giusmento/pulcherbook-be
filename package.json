{
  "name": "pulkerbook-be",
  "version": "1.0.0",
  "description": "MangoJS Microservices Monorepo for Backend Services",
  "private": true,
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "scripts": {
    "build": "pnpm -r build",
    "start:dev": "pnpm --parallel --stream -r run start:dev",
    "start:prod": "pnpm --parallel --stream -r run start:prod",
    "stop:prod": "pnpm --parallel --stream -r run stop:prod",
    "start:prod:docker": "pnpm --parallel --stream -r run start:prod:docker",
    "test": "pnpm -r test",
    "clean": "pnpm -r clean && rm -rf node_modules",
    "lint": "pnpm -r lint"
  },
  "keywords": [
    "mangojs",
    "microservices",
    "monorepo"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^24.7.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@giusmento/mangojs-core": "0.0.1-4",
    "inversify": "^7.10.2",
    "openapi-types": "^12.1.3",
    "pm2": "^6.0.13",
    "reflect-metadata": "^0.2.2"
  }
}
