    "/api/iam/v1/health": {
      "get": {
        "summary": "Get health check",
        "description": "Return a 200 status when server is up and running",
        "tags": ["Health"],
        "responses": {
          "200": {
            "description": "Server is healthy",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "ok": {
                      "type": "boolean",
                      "example": true
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/api/iam/v1/auth/admins/login": {
      "post": {
        "summary": "Login Admin users",
        "description": "Return cookies for admin users",
        "tags": ["Auth Admin"],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": ["email", "password"],
                "properties": {
                  "email": {
                    "type": "string",
                    "example": "myemail@email.com"
                  },
                  "password": {
                    "type": "string",
                    "format": "password",
                    "example": "hash256('mypassword')"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Authentication successful",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/AuthSuccessResponse"
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized"
          }
        }
      }
    },
    "/api/iam/v1/auth/admins/logout": {
      "post": {
        "summary": "Logout Admin users",
        "description": "Delete cookies and reset user connections",
        "tags": ["Auth Admin"],
        "security": [
          {
            "adminCookieAuth": []
