{
  "name": "@giusmento/pulcherbook-types",
  "version": "0.0.1-9",
  "description": "Shared types for PulcherBook backend services",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "packageManager": "pnpm@8.0.0",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./docs/*": "./src/docs/*"
  },
  "files": [
    "dist/**/*",
    "src/docs/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "pnpm run build",
    "release": "release-it",
    "publish": "pnpm publish"
  },
  "keywords": [
    "shared",
    "utilities"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "release-it": "^19.0.5",
    "typescript": "^5.9.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/giusmento/pulcherbook-be.git",
    "directory": "packages/types"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "dependencies": {
    "@giusmento/mangojs-core": "^0.0.1-12"
  }
}
